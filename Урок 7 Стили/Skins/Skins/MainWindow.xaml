<Window x:Class="Skins.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Name="mainWindow">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Skins/Skin1.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <StackPanel Style="{DynamicResource s1}" >
        <Label Style="{DynamicResource LabelStyle}" Foreground="Black" Name="lb1">Default_Skin</Label>
        <Button VerticalAlignment="Bottom" Style="{DynamicResource ButtonStyle}" Click="Button_Click">GO</Button>
    </StackPanel></Window>
